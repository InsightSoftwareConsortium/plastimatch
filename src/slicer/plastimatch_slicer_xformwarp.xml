<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Plastimatch</category>
  <title>XFORMWARP Warping with transform</title>
  <description>Universal warper</description>
  <version>0.0</version>
  <documentation-url>http://plastimatch.org</documentation-url>
  <license>BSD-style</license>
  <contributor>Please direct questions or suggestions to http://groups.google.com/group/plastimatch</contributor>
  <acknowledgements>Funding for this project has been provided by NIH 2-U54-EB005149 (NA-MIC), NIH/NCI 6-PO1 CA 21239, the Federal share of program income earned by MGH on C06CA059267, and the Progetto Rocca Foundation, a collaboration between MIT and Politecnico di Milano.</acknowledgements>
  <parameters>
    <label>Input/Output</label>
    <description>Input parameters</description>
    <image>
      <longflag>reference_vol</longflag>
      <name>plmslc_xformwarp_reference_vol</name>
      <label>Reference Volume (optional, to set size)</label>
      <channel>input</channel>
      <default>None</default>
      <description>Reference Volume (optional, to set size)</description>
    </image>
    <image>
      <longflag>plmslc_xformwarp_input_img</longflag>
      <name>plmslc_xformwarp_input_img</name>
      <label>Input Volume</label>
      <channel>input</channel>
      <default>None</default>
      <description>Input image or dose or structure set</description>
    </image>
    <transform fileExtensions=".txt" type="bspline">
      <name>plmslc_xformwarp_input_xform_s</name>
      <longflag>plmslc_xformwarp_input_xform_s</longflag>
      <description>Input transform from MRML tree</description>
      <label>Input transform (MRML)</label>
      <default>None</default>
      <channel>input</channel>
    </transform>
    <string>
      <name>plmslc_xformwarp_input_xform_f</name>
      <longflag>plmslc_xformwarp_input_xform_f</longflag>
      <description>Input transform</description>
      <label>Input transform (file)</label>
      <default>None</default>
      <channel>input</channel>
    </string>
    <image>
      <longflag>plmslc_xformwarp_output_img</longflag>
      <name>plmslc_xformwarp_output_img</name>
      <label>Output Volume</label>
      <channel>output</channel>
      <default>None</default>
      <description>Output (warped) Image etc.</description>
    </image>
  </parameters>
</executable>
